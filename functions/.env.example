# LLM Provider: 'claude' or 'openai'
LLM_PROVIDER=claude

# API Keys
# For local development, set these in .env
# For production, set via: firebase functions:config:set anthropic.key="your_key" openai.key="your_key"
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
